{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "ESNext",
        "target": "es5",
        "lib": [
            "es2015",
            "dom"
        ],
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "react",
        "allowJs": true,
        "baseUrl": "./",
        "paths": {
            "components/*": [
                "src/components/*"
            ],
        },
        "moduleResolution": "node"
    },
    "excluded": [
        "**/*.wasm"
    ]
}